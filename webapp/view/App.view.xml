<mvc:View
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    controllerName="sap.ui5.walkthrough.controller.App"
>
    <Shell>
        <App class="sapUi5AppWalkthrough">
            <pages>
                <Page title="{i18n>homePageTitle}" id="page">
                    <mvc:XMLView
                        viewName="sap.ui5.walkthrough.view.HelloPanel"
                    />
                    <mvc:XMLView
                        viewName="sap.ui5.walkthrough.view.InvoiceList"
                    />
                </Page>
            </pages>
        </App>
    </Shell>
</mvc:View>
